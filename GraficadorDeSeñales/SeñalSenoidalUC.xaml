<UserControl x:Class="GraficadorDeSeñales.SeñalSenoidalUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GraficadorDeSeñales"
             mc:Ignorable="d" Height="176.4" Width="255.5">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="0,6,0,0" TextWrapping="Wrap" Text="Amplitud" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="-1,35,0,0" TextWrapping="Wrap" Text="Fase" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="0,66,0,0" TextWrapping="Wrap" Text="Frecuencia" VerticalAlignment="Top"/>
        <TextBox x:Name="txtAmplitud" HorizontalAlignment="Left" Height="23" Margin="86,2,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtFase" HorizontalAlignment="Left" Height="23" Margin="86,34,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtFrecuencia" HorizontalAlignment="Left" Height="23" Margin="86,66,0,0" TextWrapping="Wrap" Text="1" VerticalAlignment="Top" Width="120"/>
    </Grid>
</UserControl>
